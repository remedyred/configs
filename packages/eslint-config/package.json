{
	"name": "@snickbit/eslint-config",
	"version": "1.3.8",
	"description": "Snickbit eslint config",
	"license": "MIT",
	"exports": {
		".": "./dist/configs/index.js",
		"./*": "./dist/configs/*.js"
	},
	"main": "./dist/index.js",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup --minify",
		"dev": "tsup",
		"release": "semantic-release",
		"watch": "tsup --watch"
	},
	"release": {
		"extends": [
			"../semantic-release/dist/index.js"
		]
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node"
	},
	"dependencies": {
		"@types/eslint": "8.4.10",
		"@typescript-eslint/eslint-plugin": "5.52.0",
		"@typescript-eslint/parser": "5.52.0",
		"eslint-plugin-beautiful-sort": "2.0.3",
		"eslint-plugin-jest": "27.2.0",
		"eslint-plugin-json-files": "2.1.0",
		"eslint-plugin-jsonc": "2.7.0",
		"eslint-plugin-sort-annotation": "1.0.5",
		"eslint-plugin-sort-class-members": "1.16.0",
		"eslint-plugin-unicorn": "45.0.2",
		"eslint-plugin-vue": "9.9.0",
		"eslint-plugin-yml": "1.5.0",
		"vue-eslint-parser": "9.1.0"
	},
	"devDependencies": {
		"@snickbit/node-utilities": "4.7.1",
		"@types/jest": "29.2.3",
		"@types/node": "18.11.9",
		"eslint": "8.28.0",
		"fast-glob": "3.2.12",
		"jest": "29.3.1",
		"ts-jest": "29.0.3",
		"tsup": "6.5.0",
		"typescript": "4.9.3"
	},
	"peerDependencies": {
		"eslint": ">= 8.19.0"
	},
	"engines": {
		"node": ">= 14.17"
	},
	"tsup": {
		"entry": [
			"src/**/*.ts"
		]
	}
}
