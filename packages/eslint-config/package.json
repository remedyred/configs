{
	"name": "@snickbit/eslint-config",
	"version": "1.5.4",
	"description": "Snickbit eslint config",
	"license": "MIT",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup --minify",
		"dev": "tsup",
		"release": "semantic-release",
		"watch": "tsup --watch",
		"test": "jest",
		"index": "indexer"
	},
	"release": {
		"extends": [
			"../semantic-release/dist/index.js"
		]
	},
	"jest": {
		"preset": "ts-jest/presets/default-esm",
		"testEnvironment": "node"
	},
	"dependencies": {
		"@ada-support/eslint-plugin-object-newline": "1.1.3",
		"@eslint/js": "9.5.0",
		"@snickbit/utilities": "3.13.0",
		"eslint-plugin-beautiful-sort": "3.0.1",
		"eslint-plugin-json-files": "4.2.0",
		"eslint-plugin-jsonc": "2.16.0",
		"eslint-plugin-sort-class-members": "1.20.0",
		"eslint-plugin-unicorn": "54.0.0",
		"eslint-plugin-vue": "9.26.0",
		"eslint-plugin-yml": "1.14.0",
		"jsonc-eslint-parser": "2.4.0",
		"vue-eslint-parser": "9.4.3"
	},
	"devDependencies": {
		"@eslint/compat": "1.1.0",
		"@eslint/eslintrc": "3.1.0",
		"@snickbit/indexer": "2.9.8",
		"@snickbit/node-utilities": "4.7.13",
		"@types/eslint__eslintrc": "2.1.1",
		"@types/eslint__js": "8.42.3",
		"@types/jest": "29.5.12",
		"@types/node": "20.14.6",
		"@typescript-eslint/utils": "7.13.1",
		"eslint": "9.5.0",
		"fast-glob": "3.3.2",
		"jest": "29.7.0",
		"ts-jest": "29.1.5",
		"tsup": "8.1.0",
		"typescript": "5.4.5"
	},
	"peerDependencies": {
		"eslint": ">= 9.0.0"
	},
	"engines": {
		"node": ">= 18"
	}
}
