{
	"name": "@snickbit/eslint-config",
	"version": "1.3.8",
	"description": "Snickbit eslint config",
	"license": "MIT",
	"exports": {
		".": "./dist/configs/index.js",
		"./*": "./dist/configs/*.js"
	},
	"main": "./dist/index.js",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup --minify",
		"dev": "tsup",
		"release": "semantic-release",
		"watch": "tsup --watch"
	},
	"release": {
		"extends": [
			"../semantic-release/dist/index.js"
		]
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node"
	},
	"dependencies": {
		"@ada-support/eslint-plugin-object-newline": "^1.1.3",
		"@types/eslint": "8.37.0",
		"@typescript-eslint/eslint-plugin": "5.59.5",
		"@typescript-eslint/parser": "5.59.5",
		"eslint-plugin-beautiful-sort": "3.0.1",
		"eslint-plugin-jest": "27.2.1",
		"eslint-plugin-json-files": "2.2.0",
		"eslint-plugin-jsonc": "2.8.0",
		"eslint-plugin-sort-annotation": "1.0.6",
		"eslint-plugin-sort-class-members": "1.18.0",
		"eslint-plugin-unicorn": "47.0.0",
		"eslint-plugin-vue": "9.12.0",
		"eslint-plugin-yml": "1.7.0",
		"vue-eslint-parser": "9.2.1"
	},
	"devDependencies": {
		"@snickbit/node-utilities": "4.7.10",
		"@types/jest": "29.5.1",
		"@types/node": "20.1.2",
		"eslint": "8.40.0",
		"fast-glob": "3.2.12",
		"jest": "29.5.0",
		"ts-jest": "29.1.0",
		"tsup": "6.7.0",
		"typescript": "5.0.4"
	},
	"peerDependencies": {
		"eslint": ">= 8.19.0"
	},
	"engines": {
		"node": ">= 14.17"
	},
	"tsup": {
		"entry": [
			"src/**/*.ts"
		]
	}
}
