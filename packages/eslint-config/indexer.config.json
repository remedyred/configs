			"type": "slug",
			"default": {
				"source": [
					"src/plugins/*.ts"
				],
				"type": "default"
			}
		},
		{
			"source": "src/configs/**/*",
			"output": "src/configs/index.ts",
			"type": "default",
			"default": {
				"source": [
					"src/configs/*.ts"
				],
				"type": "default"
			}
		}
	]
}
