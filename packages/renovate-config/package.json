{
	"name": "renovate-config",
	"version": "1.4.2",
	"private": true,
	"scripts": {
		"build": "pnpm run dev && pnpm run validate",
		"dev": "ts-node ./src/index.ts build",
		"release": "semantic-release",
		"validate": "renovate-config-validator ../../default.json ../../dev.json ../../dev-next.json ../../next-release.json",
		"watch": "ts-node ./src/index.ts watch"
	},
	"release": {
		"extends": [
			"../semantic-release/dist/index.js"
		]
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node"
	},
	"dependencies": {
		"@snickbit/node-cli": "3.0.24",
		"@snickbit/node-utilities": "4.7.1",
		"@snickbit/out": "2.0.48",
		"@snickbit/utilities": "3.4.17",
		"chokidar": "3.5.3",
		"debounce": "1.2.1",
		"fast-glob": "3.2.12",
		"unixify": "1.0.0"
	},
	"devDependencies": {
		"@types/jest": "29.2.3",
		"@types/node": "18.11.9",
		"jest": "29.3.1",
		"renovate": "34.28.0",
		"ts-jest": "29.0.3",
		"ts-node": "10.9.1",
		"typescript": "4.9.3"
	}
}
