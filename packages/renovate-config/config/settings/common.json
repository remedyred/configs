{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"assignees": [
		"snickbit"
	],
	"reviewers": [
		"snickbit"
	],
	"baseBranches": [
		"main",
		"next",
		"dev"
	],
	"configMigration": true,
	"prCreation": "not-pending",
	"postUpgradeTasks": {
		"commands": [
			"pnpm i --lockfile-only",
			"pnpm run fix"
		]
	},
	"rebaseWhen": "conflicted",
	"semanticCommits": "enabled",
	"dependencyDashboard": false
}
